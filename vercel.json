{
  "functions": {
    "api/webhook.js": {
      "maxDuration": 30
    },
    "api/events.js": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/events",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        },
        {
          "key": "Connection",
          "value": "keep-alive"
        },
        {
          "key": "Content-Type",
          "value": "text/event-stream"
        }
      ]
    }
  ]
}